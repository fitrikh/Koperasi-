    Private Sub simpananKaryawan()
        Dim sqlt As String
        sqlt = " select k.tgl_simpan,kj.account,k.jumlah " & _
               "from Koperasi_Simpanan k left join Koperasi_Account kj " & _
               "on kj.id_account = k.kredit " & _
               "where k.nip_anggota = '" & nipAnggota & "' order by tgl_simpan desc"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            DGVsimpanan.AutoGenerateColumns = False
            DGVsimpanan.Columns(0).DataPropertyName = dt.Columns(0).ColumnName
            DGVsimpanan.Columns(1).DataPropertyName = dt.Columns(1).ColumnName
            DGVsimpanan.Columns(2).DataPropertyName = dt.Columns(2).ColumnName
            DGVsimpanan.Columns(2).DefaultCellStyle.Format = "N0"
            DGVsimpanan.DataSource = dt
        End If
    End Sub
    Private Sub kontakkaryawan()
        Dim sqlt As String
        sqlt = " select KONTAK from KONTAK_KARYAWAN_MF " & _
               "where NIP_KARYAWAN = '" & nipAnggota & "'" & _
               "and TIPE_KONTAK = 'hp' "
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            DGVtlp.AutoGenerateColumns = False
            DGVtlp.Columns(0).DataPropertyName = dt.Columns(0).ColumnName
            DGVtlp.DataSource = dt
        End If

    End Sub
    Private Sub JenisKeanggotaan()
        Dim sqlt As String
        sqlt = "select h.idhistory, h.idanggota,h.jnsAnggota,h.tglSimpan,a.nip,a.tgl_gabung" & _
               " from Koperasi_History_Keanggotaan h " & _
               " left join Koperasi_Anggota a on a.id = h.idanggota" & _
               " where a.nip = '" & nipAnggota & "'" & _
               " and h.idhistory = (select MAX(hh.idhistory) " & _
               " from Koperasi_History_Keanggotaan hh where hh.idanggota = h.idanggota)"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            If IsDBNull(dt(0)(0)) = True Then
                TextBox1.Text = "SILVER"
                CmbJenis.SelectedIndex = 1
            ElseIf dt(0)(2) = 0 Then
                TextBox1.Text = "BRONZE"
                CmbJenis.SelectedIndex = 0
            ElseIf dt(0)(2) = 1 Then
                TextBox1.Text = "SILVER"
                CmbJenis.SelectedIndex = 1
            ElseIf dt(0)(2) = 2 Then
                TextBox1.Text = "GOLD"
                CmbJenis.SelectedIndex = 2
            ElseIf dt(0)(2) = 3 Then
                TextBox1.Text = "PLATINUM"
                CmbJenis.SelectedIndex = 3
            End If
        Else
            TextBox1.Text = "SILVER"
            CmbJenis.SelectedIndex = 1
        End If

        TextBox2.Text = Format(tgl_gabung, "dd MMMM yyyy")
        'CmbJenis.SelectedText = TextBox1.Text
    End Sub

    Private Sub pinjaman()
        Dim sqlt As String
        sqlt = "select idpinjam,jumlah_pinjam,bunga,jumlah_pinjam+bunga as jumlah_pinjaman,lama_pinjam,jumlah_bayar " & _
               "from Koperasi_Detail_Pinjaman where nip_peminjam = '" & nipAnggota & "' order by idpinjam desc"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            DGVpinjaman.AutoGenerateColumns = False
            DGVpinjaman.Columns(0).DataPropertyName = dt.Columns(0).ColumnName
            DGVpinjaman.Columns(1).DataPropertyName = dt.Columns(1).ColumnName
            DGVpinjaman.Columns(2).DataPropertyName = dt.Columns(2).ColumnName
            DGVpinjaman.Columns(3).DataPropertyName = dt.Columns(3).ColumnName
            DGVpinjaman.Columns(4).DataPropertyName = dt.Columns(4).ColumnName
            DGVpinjaman.Columns(5).DataPropertyName = dt.Columns(5).ColumnName
            DGVpinjaman.Columns(1).DefaultCellStyle.Format = "N0"
            DGVpinjaman.Columns(2).DefaultCellStyle.Format = "N0"
            DGVpinjaman.Columns(3).DefaultCellStyle.Format = "N0"
            DGVpinjaman.Columns(5).DefaultCellStyle.Format = "N0"
            DGVpinjaman.DataSource = dt
        End If
    End Sub
    Private Sub jumlahPembayaran()
        Dim idpinjam
        For i = 0 To DGVpinjaman.Rows.Count - 1
            idpinjam = DGVpinjaman.Item(0, i).Value
            Dim sqlt As String
            sqlt = "select sum(jumlah_bayar) from Koperasi_History_Angsuran where id_pinjam = '" & idpinjam & "'"
            Dim dt As New DataTable
            Dim db As New CDataBase
            dt = db.selectDB(sqlt)
            Dim jmlByr As Double
            If dt.Rows.Count > 0 Then
                If IsDBNull(dt(0)(0)) = True Then
                    jmlByr = 0
                Else
                    jmlByr = dt(0)(0)
                End If
            End If
            DGVpinjaman.Item(5, i).Value = jmlByr
        Next
    End Sub
    Private Sub ubahJenisAnggota()
        Dim sqltmax As String
        sqltmax = "Select max(idhistory)+1 as id from Koperasi_History_Keanggotaan"
        Dim dtmax As New DataTable
        Dim dbmax As New CDataBase
        dtmax = dbmax.selectDB(sqltmax)
        Dim idmax As Integer
        If dtmax.Rows.Count > 0 Then
            idmax = dtmax(0)(0)
        End If
        Dim sqltAnggota As String
        sqltAnggota = " select id from koperasi_anggota where nip = '" & nipAnggota & "'"
        Dim dtanggota As New DataTable
        Dim dbanggota As New CDataBase
        dtanggota = dbanggota.selectDB(sqltAnggota)
        If dtanggota.Rows.Count > 0 Then
            idAnggota = dtanggota(0)(0)
        End If
        If CmbJenis.SelectedIndex = 0 Then
            MsgBox("Pilhan Anggota tidak boleh kosong")
        Else
            Dim sqlt As String
            sqlt = " insert Koperasi_History_Keanggotaan" & _
                   "(idhistory,idanggota,jnsAnggota,nipPetugas,tglSimpan)" & _
                   "values('" & idmax & "','" & idAnggota & "','" & CmbJenis.SelectedIndex & "','" & nipAdmin & "','" & Format(DtUbah.Value, "yyyy-MM-dd") & "')"
            Dim db As New CDataBase
            If db.insertDB(sqlt) = True Then
                MsgBox("Penyimpanan Berhasil")
                TextBox1.Text = CmbJenis.SelectedValue
                Me.DialogResult = Windows.Forms.DialogResult.OK
            Else
                MsgBox("Gagal")
            End If
        End If

    End Sub

