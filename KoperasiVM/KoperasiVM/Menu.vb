        '    MsgBox(ketersediaanDana)
        'Else
        '    MsgBox(ketersediaanDana)
        'End If
    End Sub
    Private Sub gambar()
        Try
            Dim sql As String = "select FOTO_KARYAWAN from KARYAWAN_MF where NIP_KARYAWAN = '" & nipAnggota & "'"
            Dim oTbl As New DataTable
            Dim oConn As SqlConnection = New SqlConnection(sConn)
            Dim oCmd As SqlCommand = New SqlCommand(sql, oConn)
            Dim oDa = New SqlDataAdapter(oCmd)
            Dim oDS As New DataSet
            oDa.Fill(oDS, "VicarImage")
            oTbl = oDS.Tables(0)
            '    Dim imageData As Byte() = ""
            Dim imageData As Byte()
            If IsDBNull(oTbl.Rows(0).Item("FOTO_KARYAWAN")) = True Then
                'imageData = vbEmpty
            Else
                imageData = oTbl.Rows(0).Item("FOTO_KARYAWAN")
                Dim newImage As Image
                Using ms As MemoryStream = New MemoryStream(imageData, 0, imageData.Length)
                    ms.Write(imageData, 0, imageData.Length)
                    newImage = Image.FromStream(ms, True)
                End Using
                picLogo.Image = newImage
            End If
        Catch ex As Exception

        End Try
    End Sub

    Private Sub BtnTambahPinjam_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles BtnTambahPinjam.Click
