    Private Sub history_angsuran()
        Dim sqlt As String
        sqlt = " select k.idpinjam, isnull(H.tgl_bayar,0)as tgl_bayar, k.nip_peminjam,k.jumlah_pinjam/k.lama_pinjam as pinjam,k.bunga/k.lama_pinjam as bunga," & _
               " isnull(sum(h.jumlah_bayar),0) as bayar " & _
               " from Koperasi_Detail_Pinjaman k" & _
               " left join Koperasi_History_Angsuran h" & _
               " on k.idpinjam = h.id_pinjam" & _
               " where k.jumlah_pinjam + k.bunga <> (select isnull (SUM(hh.jumlah_bayar),0)" & _
               " from Koperasi_History_Angsuran hh where hh.id_pinjam = h.id_pinjam)" & _
               " and nip_peminjam = '" & nipAnggota & "' and h.jumlah_bayar is not null " & _
               " group by k.idpinjam,k.nip_peminjam,k.jumlah_pinjam,k.lama_pinjam,k.bunga,h.tgl_bayar "
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            DGVAngsuran.AutoGenerateColumns = False
            DGVAngsuran.Columns(0).DataPropertyName = dt.Columns(0).ColumnName
            DGVAngsuran.Columns(1).DataPropertyName = dt.Columns(1).ColumnName
            DGVAngsuran.Columns(2).DataPropertyName = dt.Columns(2).ColumnName
            DGVAngsuran.Columns(3).DataPropertyName = dt.Columns(3).ColumnName
            DGVAngsuran.Columns(4).DataPropertyName = dt.Columns(4).ColumnName
            DGVAngsuran.Columns(5).DataPropertyName = dt.Columns(5).ColumnName
            DGVAngsuran.Columns(6).DataPropertyName = dt.Columns(5).ColumnName
            DGVAngsuran.DataSource = dt
        End If
    End Sub
    Private Sub HIStory_angsuran2()
        DGVAngsuran.Rows.Clear()
        id_pinjam = DGVpinjaman.Item(0, DGVpinjaman.CurrentRow.Index).Value
        lama_pinjam = DGVpinjaman.Item(4, DGVpinjaman.CurrentRow.Index).Value
        Dim sqlt As String
        sqlt = " select a.tgl_bayar FROM " & _
               " Koperasi_History_Angsuran a where a.id_pinjam = '" & id_pinjam & "' and a.jns_bayar = '102'"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        Dim tgl_bayar
        Dim pokok
        Dim bunga
        'Dim jns_bayar
        Dim jml_bayar
        Dim bayar
        Dim id_history
        For i = 0 To dt.Rows.Count - 1
            tgl_bayar = dt(i)(0)
            Dim sqlt2 As String
            sqlt2 = " select a.id_history, a.tgl_bayar,a.jumlah_bayar, a.via_bayar,a.jns_bayar FROM " & _
                   " Koperasi_History_Angsuran a where a.tgl_bayar = '" & tgl_bayar & "' and a.jns_bayar = '102' and a.id_pinjam = '" & id_pinjam & "' "
            Dim dt2 As New DataTable
            Dim db2 As New CDataBase
            dt2 = db2.selectDB(sqlt2)
            pokok = dt2(0)(2)
            bayar = dt2(0)(3)
            id_history = dt2(0)(0)
            Dim sqlt3 As String
            sqlt3 = " select a.id_history, a.tgl_bayar,a.jumlah_bayar, a.via_bayar,a.jns_bayar FROM " & _
                   " Koperasi_History_Angsuran a where a.tgl_bayar = '" & tgl_bayar & "' and a.jns_bayar = '500' and a.id_pinjam = '" & id_pinjam & "'"
            Dim dt3 As New DataTable
