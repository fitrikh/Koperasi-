            Dim pinjam As New Pinjaman
            pinjam.nippeminjam = nipAnggota
            pinjam.namapeminjam = namaAnggota
            If sisapinjaman = 0 Then
                'If ketersediaanDana > 0 Then
                'MsgBox(ketersediaanDana)
                If pinjam.ShowDialog = Windows.Forms.DialogResult.OK Then
                    TxtNama.Text = namaAnggota
                    TxtNip.Text = nipAnggota
                    simpananKaryawan()
                    kontakkaryawan()
                    pinjaman()
                    jumlahsimpanan()
                    angsuran()
                End If
                'Else
                MsgBox("Ketersedian Dana Tidak Mencukupi mohon hubungi managemen")
                'End If
                'MsgBox(ketersediaanDana)
            Else
                MsgBox("Mohon maaf Anggota belum bisa meminjam " & vbCrLf & "karena masih ada tanggungan" & vbCrLf & "Hubungi Petugas")
            End If
        Else
            MsgBox("Mohon maaf Anggota belum bisa meminjam " & vbCrLf & "karena belum memenuhi persyaratan" & vbCrLf & "Hubungi Petugas")
        End If

    End Sub


