            dt3 = db3.selectDB(sqlt3)
            If dt3.Rows.Count > 0 Then
                bunga = dt3(0)(2)
                jml_bayar = pokok + bunga
                DGVAngsuran.Rows.Add(id_history, Format(tgl_bayar, "dd-MMM-yy"), id_pinjam, pokok, bunga, jml_bayar, bayar)
            End If
        Next

