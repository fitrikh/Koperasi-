    Private Sub ceksimpananpokok()
        Dim sqlt As String
        sqlt = " select SUM(jumlah) from Koperasi_Simpanan " & _
                "where nip_anggota = '" & nipAnggota & "' and id_jns_simpanan = 1"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            If IsDBNull(dt(0)(0)) = True Then
                simpananpokok = 0
            Else
