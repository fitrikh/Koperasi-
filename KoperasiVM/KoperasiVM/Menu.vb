    Private Sub cekpinjamanTersisa()
        Dim sqlt As String
        sqlt = " select (jumlah_pinjam+bunga)-jumlah_bayar as sisaPinjaman" & _
               " from Koperasi_Detail_Pinjaman where nip_peminjam = '" & nipAnggota & "'"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            sisapinjaman = dt(0)(0)
        End If
    End Sub
    Private Sub cekKetersediaanDana()
        Dim sqlt As String
        sqlt = "select SUM(jumlah)as totalsimpanan from Koperasi_Simpanan"
        Dim dt As New DataTable
        Dim db As New CDataBase
        dt = db.selectDB(sqlt)
        If dt.Rows.Count > 0 Then
            simpananAnggota = dt(0)(0)
        End If

        Dim sqlt2 As String
        sqlt2 = "select SUM(jumlah_bayar) from Koperasi_Detail_Pinjaman"
        Dim dt2 As New DataTable
        Dim db2 As New CDataBase
        dt2 = db2.selectDB(sqlt2)
        If dt2.Rows.Count > 0 Then
            angsuranAnggota = dt2(0)(0)
        End If

        Dim sqlt3 As String
        sqlt3 = "select SUM(jml) from koperasi_shu"
        Dim dt3 As New DataTable
        Dim db3 As New CDataBase
        dt3 = db3.selectDB(sqlt3)
        If dt3.Rows.Count > 0 Then
            SHUAnggota = dt3(0)(0)
        End If

        Dim sqlt4 As String
        sqlt4 = "select SUM(jumlah) from Koperasi_Modal where status = 1"
        Dim dt4 As New DataTable
        Dim db4 As New CDataBase
        dt4 = db4.selectDB(sqlt4)
        If dt4.Rows.Count > 0 Then
            modalkoperasi = dt4(0)(0)
        End If

        Dim sqlt5 As String
        sqlt5 = "select SUM(jumlah_pinjam) from Koperasi_Detail_Pinjaman"
        Dim dt5 As New DataTable
        Dim db5 As New CDataBase
        dt5 = db5.selectDB(sqlt5)
        If dt5.Rows.Count > 0 Then
            pinjamanAnggota = dt5(0)(0)
        End If
        ketersediaanDana = (simpananAnggota + angsuranAnggota + modalkoperasi + SHUAnggota) - pinjamanAnggota
